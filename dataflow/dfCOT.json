{
	"name": "dfCOT",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Excel1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Output3",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "AzureDataLakeStorage1",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "filter2"
				},
				{
					"name": "select1"
				},
				{
					"name": "pivot1"
				},
				{
					"name": "ModifyColumns1",
					"description": "Generado automáticamente por acciones de vista previa de datos"
				},
				{
					"name": "ModifyColumns2",
					"description": "Generado automáticamente por acciones de vista previa de datos"
				},
				{
					"name": "pivot2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string,",
				"          Column_7 as string,",
				"          Column_8 as string,",
				"          Column_9 as string,",
				"          Column_10 as string,",
				"          Column_11 as string,",
				"          Column_12 as string,",
				"          Column_13 as string,",
				"          Column_14 as string,",
				"          {1} as string,",
				"          {2} as string,",
				"          {3} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(((greater(Column_1, '2017') && (notEquals(Column_1,'Mes') && notEquals(Column_1,'Acumulado')))  && notEquals(Column_1, 'Diferencia año anterior')) && notEquals(Column_1, 'Mensual')) ~> filter2",
				"filter2 select(mapColumn(",
				"          Column_1,",
				"          Column_2,",
				"          Column_3,",
				"          Column_4,",
				"          Column_5,",
				"          Column_6,",
				"          Column_7,",
				"          Column_8,",
				"          Column_9,",
				"          Column_10,",
				"          Column_11,",
				"          Column_12,",
				"          Column_13",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"ModifyColumns2 pivot(groupBy(Column_1,",
				"          Column_2,",
				"          Column_3,",
				"          Column_4,",
				"          Column_5,",
				"          Column_6,",
				"          Column_7,",
				"          Column_8,",
				"          Column_9,",
				"          Column_10,",
				"          Column_11,",
				"          Column_12,",
				"          Column_13),",
				"     pivotBy(Column_2),",
				"     Enero = sum(toFloat(Column_2)),",
				"          Febrero = sum(toFloat(Column_3)),",
				"     columnNaming: '$N$V',",
				"     lateral: false) ~> pivot1",
				"select1 derive(Column_2 = toDecimal(Column_2),",
				"          Column_4 = toDecimal(Column_4)) ~> ModifyColumns1",
				"ModifyColumns1 derive(Column_3 = toDecimal(Column_3),",
				"          Column_5 = toDecimal(Column_5),",
				"          Column_6 = toDecimal(Column_6),",
				"          Column_7 = toDecimal(Column_7),",
				"          Column_8 = toDecimal(Column_8),",
				"          Column_9 = toDecimal(Column_9),",
				"          Column_10 = toDecimal(Column_10),",
				"          Column_11 = toDecimal(Column_11),",
				"          Column_12 = toDecimal(Column_12),",
				"          Column_13 = toDecimal(Column_13)) ~> ModifyColumns2",
				"pivot1 pivot(groupBy(Column_1),",
				"     pivotBy(Column_2),",
				"     Mes = sum(Column_2),",
				"     columnNaming: '$N$V',",
				"     lateral: true) ~> pivot2",
				"pivot2 sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     input(",
				"          Column1 as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     mapColumn(",
				"          Column1 = Column_1",
				"     )) ~> sink1"
			]
		}
	}
}